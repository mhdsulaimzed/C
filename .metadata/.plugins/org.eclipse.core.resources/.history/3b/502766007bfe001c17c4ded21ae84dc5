#include <stdio.h>
#include <stdlib.h>

int main(void) {
	int i,j,a[100],flag=0,limit;
	printf("enter the limit");
	scanf("%d",&limit);
	printf("enter the values of the array");
	for(i=0;i<limit;i++)
	{
		scanf("%d",&a[i]);
	}
	for(i=0;i<limit;i++)
	{
		for(j=0;j<limit;j++)
		{
			if(i!=j&&a[i]==a[j])
			{

					flag++;
				}

flag=0;

			if(flag==0)
							{
							printf("%d",a[i]);
							}
		}
	}

	return EXIT_SUCCESS;
}
